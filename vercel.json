{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/consolidated/(.*)",
      "dest": "/api/consolidated.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/consolidated.js"
    },
    {
      "src": "/api/quiz/(.*)",
      "dest": "/api/consolidated.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/consolidated.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/q/([^/]+)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}