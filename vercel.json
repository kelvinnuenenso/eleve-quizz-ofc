{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "consolidated-server.cjs",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/consolidated/(.*)",
      "dest": "/consolidated-server.cjs"
    },
    {
      "src": "/api/health",
      "dest": "/consolidated-server.cjs"
    },
    {
      "src": "/api/quiz/(.*)",
      "dest": "/consolidated-server.cjs"
    },
    {
      "src": "/api/(.*)",
      "dest": "/consolidated-server.cjs"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/q/([^/]+)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "consolidated-server.cjs": {
      "maxDuration": 30
    }
  }
}